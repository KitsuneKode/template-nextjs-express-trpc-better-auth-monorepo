{
  "name": "@template/worker",
  "module": "src/index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "tsdown src/index.ts",
    "check-types": "tsc -b",
    "compile": "bun build --compile --minify --sourcemap --bytecode ./src/index.ts --outfile server",
    "dev": "bun run --hot src/index.ts",
    "start": "bun run dist/index.js",
    "lint": "bun run lint"
  },
  "devDependencies": {
    "@template/typescript-config": "*",
    "tsdown": "^0.15.4",
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@template/backend-common": "*",
    "@template/common": "*",
    "@template/auth": "*",
    "@template/store": "*"
  }
}
