{
  "name": "@template/worker",
  "module": "src/index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "check-types": "tsc -noEmit",
    "build": "bun build ./src/index.ts --outdir ./dist --target bun --minify --sourcemap",
    "dev": "bun run --hot src/index.ts",
    "start": "bun run dist/server.js",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix"
  },
  "devDependencies": {
    "@types/bun": "1.3.9",
    "eslint": "^10.0.0",
    "@template/eslint-config": "workspace:*",
    "@template/typescript-config": "workspace:*"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@template/backend-common": "workspace:*",
    "@template/common": "workspace:*",
    "@template/auth": "workspace:*",
    "@template/store": "workspace:*"
  }
}
