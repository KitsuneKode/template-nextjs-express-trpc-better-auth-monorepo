{
  "name": "@template/worker",
  "module": "src/index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "check-types": "tsc -noEmit",
    "build": "bun build ./src/index.ts --outdir ./dist --target bun --minify --sourcemap",
    "dev": "bun run --hot src/index.ts",
    "start": "bun run dist/server.js",
    "lint": "bun run lint"
  },
  "devDependencies": {
    "@template/typescript-config": "*",
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@template/backend-common": "*",
    "@template/common": "*",
    "@template/auth": "*",
    "@template/store": "*"
  }
}
