    "eslint": "^9.0.0",
    "typescript": "^5.9.3",
    "@tailwindcss/postcss": "^4.2.1",
    "@template/eslint-config": "workspace:*",
    "@template/typescript-config": "workspace:*"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@base-ui/react": "^1.2.0",
    "class-variance-authority": "^0.7.1",
