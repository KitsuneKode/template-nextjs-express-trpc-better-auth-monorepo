    "@hugeicons/react": "^1.1.5",
    "motion": "^12.34.3",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-hook-form": "^7.71.2",
    "tailwind-merge": "^3.5.0",
    "tailwindcss": "^4.2.1",
    "shadcn": "^3.8.5",
    "tw-animate-css": "^1.4.0",
    "zod": "^4.1.11"
